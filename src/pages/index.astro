---
import Features2 from '~/components/widgets/Features2.astro';
import Layout from '~/layouts/PageLayout.astro';

const metadata = {
  title: '14822579 Canada Corp.',
};

const items = [
  {
    title: 'AdaNomad',
    description:
      'Vibrant consulting from experts collaborate to provide valuable insights and solutions to businesses.',
    icon: 'tabler:users',
    url: 'https://adanomad.com',
  },
  {
    title: 'AceTeam',
    description:
      'A private AI platform for businesses, research, and academia to interact with documents using natural language. Ask questions, get answers, and speed up discoveries.',
    icon: 'tabler:message',
    url: 'https://aceteam.ai',
  },
  {
    title: 'BJJ Ace',
    description:
      'Improve your Jiujitsu using AI! Get AI-guided feedback on your techniques and access expert tutorials from professionals.',
    icon: 'tabler:stretching',
    url: 'https://bjjace.com',
  },
];
---

<Layout metadata={metadata}>
  <!-- Features2 Widget ************** -->

  <Features2
    title="Our Products"
    subtitle="Empowering businesses with technology to improve efficiency and productivity."
    columns={3}
    items={items}
  />
</Layout>
